root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
# editorconfig is unable to ignore longs strings or urls
max_line_length = null

[*.js]
block_comment_start = /*
block_comment = *
block_comment_end = */

[LICENSE]
indent_size = ignore
